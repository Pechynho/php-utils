{
  "name": "pechynho/php-utils",
  "description": "This is a small collection of PHP libraries for easier working with strings, arrays, files, scalars, formatting and accessing properties.",
  "type": "library",
  "minimum-stability": "stable",
  "homepage": "https://github.com/Pechynho/php-utils",
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Pech",
      "email": "pechynho@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-mbstring": "*",
    "symfony/property-access": "^5.1",
    "doctrine/annotations": "^1.10"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0 <8.0",
    "vladahejda/phpunit-assert-exception": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Pechynho\\Utility\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pechynho\\Test\\": "tests/"
    }
  }
}
