{
  "name": "pechynho/php-utils",
  "description": "This is a small collection of PHP libraries for easier working with strings, arrays, files, scalars, formatting and accessing properties.",
  "type": "library",
  "minimum-stability": "stable",
  "homepage": "https://github.com/Pechynho/php-utils",
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Pech",
      "email": "pechynho@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-mbstring": "*",
    "symfony/property-access": "^3.4",
    "doctrine/annotations": "^1.4",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "vladahejda/phpunit-assert-exception": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Pechynho\\Utility\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pechynho\\Test\\": "tests/"
    }
  }
}
