{
  "name": "pechynho/php-utils",
  "description": "This is a small collection of PHP libraries for easier working with strings, arrays, files, scalars, formatting and accessing properties.",
  "type": "library",
  "minimum-stability": "stable",
  "homepage": "https://github.com/Pechynho/php-utils",
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Pech",
      "email": "pechynho@gmail.com"
    }
  ],
  "config": {
    "platform": {
      "php": "5.6.40"
    }
  },
  "require": {
    "php": "^5.6",
    "ext-mbstring": "*",
    "symfony/property-access": "3.4.35",
    "doctrine/annotations": "1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.27",
    "vladahejda/phpunit-assert-exception": "1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Pechynho\\Utility\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pechynho\\Test\\": "tests/"
    }
  }
}
